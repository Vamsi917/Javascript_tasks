let a = 123456;
a=String(a)
let aLen=a.length

let reverseA=""
for(let i in a){
    reverseA+=a[a.length-i-1]
}
console.log(reverseA)

//second program to reverse a String
let b = "abcdefg";
let bLen=b.length

let reverseB=""
for(let i in b){
    reverseB+=b[b.length-i-1]
}
console.log(reverseB)